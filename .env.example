# PostgreSQL DB 연결 정보
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=your_db_user
DB_PASSWORD=your_db_password
DB_DATABASE=kimp_db

# Telegram 봇 정보
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
TELEGRAM_CHAT_ID=your_telegram_chat_id (알림 받을 채팅 ID)

# 차익거래 전략 설정
INITIAL_CAPITAL_KRW=1500000                     # 초기 자본금 (KRW)
PROFIT_THRESHOLD_PERCENT=0.7                    # 고프리미엄 진입 기준 최소 순수익률 (%)
TARGET_OVERALL_CYCLE_PROFIT_PERCENT=0.1         # 전체 차익거래 사이클 목표 순수익률 (%)

# 저프리미엄 탐색 전략 설정
LOW_PREMIUM_INITIAL_TARGET_PROFIT_RATE_PERCENT=0.3 # 저프 탐색 초기 목표 수익률 (%)
LOW_PREMIUM_MINIMUM_ACCEPTABLE_PROFIT_RATE_PERCENT=0.05 # 저프 탐색 최소 허용 수익률 (%)
LOW_PREMIUM_TARGET_ADJUSTMENT_INTERVAL_MS=3600000    # 저프 목표 수익률 하향 조정 간격 (ms)
LOW_PREMIUM_TARGET_ADJUSTMENT_STEP_PERCENT=0.05    # 저프 목표 수익률 하향 조정 폭 (%)
LOW_PREMIUM_MAX_SEARCH_DURATION_MS=86400000        # 저프 최대 탐색 기간 (ms)

# 거래소 수수료율 (필요시 추가 및 FeeCalculatorService에서 사용하도록 수정)
# UPBIT_SPOT_TAKER_FEE_RATE=0.0005
# BINANCE_SPOT_TAKER_FEE_RATE=0.001

# API 키 (필요시)
# UPBIT_API_KEY=
# UPBIT_API_SECRET=
# BINANCE_API_KEY=
# BINANCE_API_SECRET=

# 로깅 레벨 (예: 'log', 'error', 'warn', 'debug', 'verbose')
LOG_LEVEL=debug
NODE_ENV=development